
!################################################
!            Xresources Configuration           #
!################################################


!############# Native transparency ##############

URxvt*inheritPixmap: true
URxvt*transparent:   true
URxvt*shading:       35


!############### Terminal Colors ################

*background:#222222
*foreground:#e8e9ca

*color0:    #222222
*color1:    #8b3e2f
*color2:    #526f33
*color3:    #665847
*color4:    #4a708b
*color5:    #7a378b
*color6:    #528b8b
*color7:    #999999

*color8:    #828282
*color9:    #d75f00
*color10:   #cee318
*color11:   #eee685
*color12:   #9ac0cd
*color13:   #9f79ee
*color14:   #79cdcd
*color15:   #e8e9ca


!############# Scrollbar & Cursor ################

URxvt*scrollBar: false
URxvt*internalBorder: 0
URxvt*externalBorder: 0
URxvt*geometry: 100x100+0+0

URxvt*saveLines:	10000
URxvt*cursorBlink:	true
URxvt*cursorUnderline:	true
URxvt*cursorColor:	#5ba8d8


##################### Fonts ######################

URxvt.font:	xft:Monospace:pixelsize=24:antialias=true:autohint=true
URxvt.boldFont:	xft:Monospace:bold:pixelsize=24:antialias=true:autohint=true
URxvt*lineSpace: 0
URxvt*letterSpace: 0
